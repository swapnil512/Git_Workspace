<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
  <!-- Declare properties files. Note that "messages1" for base-name really
       means WEB-INF/classes/messages1.properties (src/messages1.properties in
       the Eclipse project) -->
  <application>
    <resource-bundle>
      <base-name>messages1</base-name>
      <var>msgs1</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>messages2</base-name>
      <var>msgs2</var>
    </resource-bundle>
    
    <resource-bundle>
      <base-name>winners</base-name>
      <var>winMessages</var>
    </resource-bundle>
  </application>
</faces-config>
